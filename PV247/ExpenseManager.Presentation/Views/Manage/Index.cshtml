@model ExpenseManager.Presentation.Models.ManageViewModels.IndexViewModel


@section title
{
    Manage your account
}

<h2>Manage your account</h2>
<p class="text-success">@Model.StatusMessage</p>

<div>
    <h4>Change your account settings</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>External Logins:</dt>
        <dd>

            @Model.Logins.Count <a asp-controller="Manage" asp-action="ManageLogins" class="btn-bracketed">Manage</a>
        </dd>
      
    </dl>
</div>
